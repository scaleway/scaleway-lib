{
  "name": "@scaleway/use-random-name",
  "version": "1.0.1",
  "description": "A small react utility to generate a random name",
  "engines": {
    "node": ">=20.x"
  },
  "sideEffects": false,
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "type:generate": "tsc --declaration -p tsconfig.build.json",
    "build": "vite build --config vite.config.ts && pnpm run type:generate",
    "build:profile": "npx vite-bundle-visualizer -c vite.config.ts",
    "lint": "eslint --report-unused-disable-directives --cache --cache-strategy content --ext ts,tsx ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scaleway/scaleway-lib",
    "directory": "packages/use-random-name"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "@scaleway/use-segment": "workspace:*"
  },
  "dependencies": {
    "@scaleway/random-name": "workspace:*"
  }
}
