{
  "name": "@scaleway/use-storage",
  "version": "3.1.6",
  "description": "A React hook to interact with local/session storage in reactive way",
  "engines": {
    "node": ">=20.x"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "typecheck": "tsc --noEmit",
    "typecheck:go": "tsgo --noEmit",
    "type:generate": "tsc --declaration -p tsconfig.build.json",
    "type:generate:go": "tsgo --declaration -p tsconfig.build.json",
    "build": "vite build --config vite.config.ts && pnpm run type:generate",
    "build:profile": "npx vite-bundle-visualizer -c vite.config.ts",
    "test:unit": "vitest --run --config vite.config.ts",
    "test:unit:coverage": "pnpm test:unit --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scaleway/scaleway-lib",
    "directory": "packages/use-storage"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0"
  },
  "devDependencies": {
    "react": "catalog:"
  }
}
