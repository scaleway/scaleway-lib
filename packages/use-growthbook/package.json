{
  "name": "@scaleway/use-growthbook",
  "version": "2.1.7",
  "description": "Utility package to expose AB test tool",
  "engines": {
    "node": ">=20.x"
  },
  "main": "./dist/index.cjs",
  "sideEffects": false,
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "typecheck": "tsc --noEmit",
    "type:generate": "tsc --declaration -p tsconfig.build.json",
    "build": "vite build --config vite.config.ts && pnpm run type:generate",
    "build:profile": "npx vite-bundle-visualizer -c vite.config.ts",
    "lint": "eslint --report-unused-disable-directives --cache --cache-strategy content --ext ts,tsx .",
    "test:unit": "vitest --run --config vite.config.ts",
    "test:unit:coverage": "pnpm test:unit --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scaleway/scaleway-lib",
    "directory": "packages/growthbook"
  },
  "license": "MIT",
  "keywords": [
    "ab",
    "feature flags"
  ],
  "dependencies": {
    "@growthbook/growthbook-react": "1.3.1"
  },
  "devDependencies": {
    "react": "19.0.0"
  },
  "peerDependencies": {
    "react": "18.x || 19.x"
  }
}
